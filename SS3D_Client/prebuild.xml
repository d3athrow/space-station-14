<Project frameworkVersion="v4_0" name="SpaceStation13" rootNamespace="SS13" type="WinExe" path="SS3D_Client">
  <Configuration name="Debug">
    <Options>
      <OutputPath>../Bin/Client</OutputPath>
    </Options>
  </Configuration>
  <Configuration name="Release">
    <Options>
      <OutputPath>../Bin/Client</OutputPath>
    </Options>
  </Configuration>
  <ReferencePath>../Bin</ReferencePath>
  <ReferencePath>../Third-Party</ReferencePath>
  <Reference name="Gorgon" localCopy="true" path="../Third-Party"/>
  <Reference name="GorgonInput" localCopy="true" path="../Third-Party"/>
  <Reference name="Microsoft.Diagnostics.Runtime"/>
  <Reference name="System"/>
  <Reference name="System.Core"/>
  <Reference name="System.Drawing"/>
  <Reference name="System.Windows.Forms"/>
  <Files>
    <File buildAction="Compile" subType="Form">MainWindow.cs</File>
    <File buildAction="Compile">MainWindow.Designer.cs</File>
    <!--<File buildAction="EmbeddedResource">MainWindow.resx</File>-->
    <File buildAction="Compile">Program.cs</File>
    <File buildAction="Compile">Properties/AssemblyInfo.cs</File>
    <File buildAction="Compile">Properties/Resources.Designer.cs</File>
    <File buildAction="None">app.config</File>
	<File buildAction="Content" copyToOutput="Always">../Third-Party/ClrDump.dll</File>
    <File buildAction="Content" copyToOutput="PreserveNewest">EntityTemplates/Atmos.xml</File>
